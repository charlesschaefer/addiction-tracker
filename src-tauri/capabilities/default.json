{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "context": "local",
  "windows": [
    "main",
    "webview"
  ],
  "permissions": [
    "core:app:default",
    "notification:default",
    "http:default",
    "core:event:allow-listen",
    "core:window:allow-is-focused",
    "core:window:allow-is-visible"
  ]
}